    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@next/eslint-plugin-next": "^15.2.4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "19.1.0",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.24.0",
    "eslint-config-next": "^15.2.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.17",
    "typescript": "5.8.3"
  }
